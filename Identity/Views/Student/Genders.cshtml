@model IEnumerable<GenderVM>;
@{
    ViewData["Title"] = "List - Gender";
}
<div class=mt-3  style="color:#343a40;display: flex;justify-content: end;">
    <a asp-controller="Student" asp-action="Gender" class="btn btn-primary ml-4">Add Gender</a>
</div>
<hr />
<section>
    <table class="table table-striped">
        <tr>
            <th>Name</th>
            <th>Actions</th>
        </tr>
        @if (Model.Any())
        {
          @foreach (var item in Model)
         {
               <tr>
                 
                    <td>@item.Name</td>
                    <td>
                        <a href="#">Edit</a> |
                        <a href="#">Delete</a> |
                        <a href="#">Details</a> |
                    </td>
                </tr>
         }
        }
    </table>
</section>
